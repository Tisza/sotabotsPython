import javax.swing.JTextField;

import edu.wpi.first.smartdashboard.gui.StaticWidget;
import edu.wpi.first.smartdashboard.properties.Property;


public class Inputs extends StaticWidget {
	static JTextField red;
	static JTextField green;
	static JTextField blue;
	
	@Override
	public void init() {
		red    = new JTextField("RED");
		green  = new JTextField("GREEN");
		blue   = new JTextField("BLUE");
		
		red.setEditable(true);
		green.setEditable(true);
		blue.setEditable(true);
		
		add(red);
		add(green);
		add(blue);
		
	}

	@Override
	public void propertyChanged(Property arg0) {
		
	}

}
