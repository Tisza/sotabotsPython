import java.util.Scanner;
import java.util.Scanner.*;

public class Test {
	public static void main( String args[] ) {
		double[] sampleArray = new double[10];
		int i = 0;
		
		while (sampleArray.length > 10 && sampleArray[9] < 10) {
			sampleArray[i]= i;
			i++;
			System.out.println(sampleArray[i]);
		}
		double sum = 0;
		
		for ( int q = 0; q < sampleArray.length; q++ ) {
			sampleArray[q] = q;
			sum += sampleArray[q];
			System.out.println("Sum: " + sum);
		}
		System.out.println(sum/sampleArray.length);
		
		
	}
}
