import java.util.Scanner;
import java.util.Scanner.*;

public class Test {
	public static void main( String args[] ) {
		double[] sampleArray = new double[10];
		int i = 0;
		
		while (sampleArray.length > 10 && sampleArray[9] < 10) {
			sampleArray[i]= i;
			i++;
			System.out.println(sampleArray[i]);
		}
		double sum = 0;
		
		for ( int i = 0; i < sampleArray.length; i++ ) {
			sum += sampleArray[i];
		}
		
		
	}
}
